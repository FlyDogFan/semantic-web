    <adms:status rdf:resource="http://purl.org/adms/status/Completed"/>
  </adms:SemanticDistribution>

  <foaf:Person rdf:about="http://vocab.deri.ie/wsapo#owesac">
    <foaf:name>Owen Sacco</foaf:name>
    <foaf:homepage rdf:resource="http://www.deri.ie/about/team/member/owen_sacco/"/>
  </foaf:Person>

  <dcterms:Agent rdf:about="http://vocab.deri.ie/wsapo#DERI%2C%20NUI%20Galway">
