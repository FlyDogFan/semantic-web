  <rdfs:Class rdf:about="http://vocab.deri.ie/wsapo#WebService">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Web Service</rdfs:label>
    <rdfs:comment>The main class of WSAPO.</rdfs:comment>
  </rdfs:Class>

  <rdf:Property rdf:about="http://vocab.deri.ie/wsapo#hasCredentials">
    <rdfs:label>has credentials</rdfs:label>
    <rdfs:comment>A property that defines the client's credentials (generated by a particular Web Service) which are used during the authorisation process. The credentials are described using the Credentials Ontology (CO).</rdfs:comment>
    <rdfs:domain rdf:resource="http://vocab.deri.ie/wsapo#WebService"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://vocab.deri.ie/wsapo#hasTemporaryTokenEndpoint">
    <rdfs:label>has temporary token endpoint</rdfs:label>
    <rdfs:comment>This property defines the Web Service's endpoint for generating the request (temporary) token credentials.</rdfs:comment>
    <rdfs:domain rdf:resource="http://vocab.deri.ie/wsapo#WebService"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://vocab.deri.ie/wsapo#hasAccessTokenEndpoint">
    <rdfs:label>has access token endpoint</rdfs:label>
    <rdfs:comment>A property that defines the Web Service's endpoint to exchange the request (temporary) token credentials to access token credentials.</rdfs:comment>
    <rdfs:domain rdf:resource="http://vocab.deri.ie/wsapo#WebService"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://vocab.deri.ie/wsapo#hasAuthoriseEndpoint">
    <rdfs:label>has authorise endpoint</rdfs:label>
    <rdfs:comment>A property that defines the Web Service's authorisation endpoint.</rdfs:comment>
    <rdfs:domain rdf:resource="http://vocab.deri.ie/wsapo#WebService"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdf:Property>

</rdf:RDF>