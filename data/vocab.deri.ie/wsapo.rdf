  <owl:Ontology rdf:about="http://vocab.deri.ie/wsapo">
    <rdf:type rdf:resource="http://www.w3.org/ns/adms#SemanticAsset"/>
    <dcterms:title>Web Service Authorisation Preferences Ontology (WSAPO)</dcterms:title>
    <dcterms:description>The Web Service Authorisation Preferences Ontology (WSAPO) is a light weight vocabulary that describes the details of the Web Service Authorisation component which are stored on the client side. These details are used by the client during the authorisation process.</dcterms:description>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-05-02</dcterms:modified>
    <vann:preferredNamespaceUri>http://vocab.deri.ie/wsapo#</vann:preferredNamespaceUri>
    <vann:preferredNamespacePrefix>wsapo</vann:preferredNamespacePrefix>
    <foaf:homepage rdf:resource="http://vocab.deri.ie/wsapo.html"/>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-05-02</dcterms:created>
    <dcterms:publisher>http://vocab.deri.ie/wsapo#DERI%2C%20NUI%20Galway</dcterms:publisher>
    <dcterms:partOf rdf:resource="http://vocab.deri.ie"/>
    <dcterms:type rdf:resource="http://purl.org/adms/assettype/Ontology"/>
    <dcterms:status rdf:resource="http://purl.org/adms/status/UnderDevelopment"/>
    <dc:creator rdf:resource="http://vocab.deri.ie/wsapo#owesac"/>
  </owl:Ontology>

