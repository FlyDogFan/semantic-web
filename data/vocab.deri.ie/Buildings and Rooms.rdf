    <adms:accessURL>http://vocab.deri.ie/rooms.ttl</adms:accessURL>
    <adms:status rdf:resource="http://purl.org/adms/status/Completed"/>
  </adms:SemanticDistribution>

  <adms:SemanticDistribution rdf:about="http://vocab.deri.ie/rooms#rdf">
    <dcterms:FileFormat></dcterms:FileFormat>
    <adms:accessURL>http://vocab.deri.ie/rooms.rdf</adms:accessURL>
    <adms:status rdf:resource="http://purl.org/adms/status/Completed"/>
  </adms:SemanticDistribution>

  <foaf:Person rdf:about="http://vocab.deri.ie/rooms#cygri">
