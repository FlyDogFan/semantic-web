  </foaf:Person>

  <dcterms:Agent rdf:about="http://vocab.deri.ie/rooms#Digital%20Enterprise%20Research%20Institute%2C%20NUI%20Galway">
    <foaf:member rdf:resource="http://vocab.deri.ie/rooms#cygri"/>
    <foaf:name>Digital Enterprise Research Institute, NUI Galway</foaf:name>
    <foaf:homepage rdf:resource="http://www.deri.ie/"/>
  </dcterms:Agent>

  <rdfs:Class rdf:about="http://vocab.deri.ie/rooms#Building">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Building</rdfs:label>
    <rdfs:label xml:lang="zh-CN">建筑物</rdfs:label>
    <rdfs:comment xml:lang="en">A human-made structure used for sheltering or continuous occupancy.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">用于庇护或长期居住的人造结构。</rdfs:comment>
    <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
    <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://vocab.deri.ie/rooms#Room">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Room</rdfs:label>
    <rdfs:label xml:lang="zh-CN">房间</rdfs:label>
    <rdfs:comment xml:lang="en">A space inside a structure, typically separated from the outside by exterior walls and from other rooms in the same structure by internal walls.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">构造内部的空间，通常用外墙与外部隔开，用内墙与同一构造中的其他房间隔开。</rdfs:comment>
    <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
    <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://vocab.deri.ie/rooms#Floor">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Floor</rdfs:label>
    <rdfs:label xml:lang="zh-CN">楼层</rdfs:label>
    <rdfs:comment xml:lang="en">A level part of a building that has a permanent roof. A storey of a building. </rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">建筑物的一级部分，它有永久性的屋顶。建筑物的一个楼层。</rdfs:comment>
    <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
    <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://vocab.deri.ie/rooms#FloorSection">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Floor Section</rdfs:label>
    <rdfs:label xml:lang="zh-CN">楼层部分</rdfs:label>
    <rdfs:comment xml:lang="en">A named part of a floor of a building. Typically used to denote several rooms that are grouped together based on spatial arrangement or use.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">建筑物楼层的命名部分。通常用来表示基于空间安排或使用组合在一起的几个房间。</rdfs:comment>
    <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://vocab.deri.ie/rooms#Desk">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Desk</rdfs:label>
    <rdfs:label xml:lang="zh-CN">桌子</rdfs:label>
    <rdfs:comment xml:lang="en">A table used in a work or office setting, typically for reading, writing, or computer use.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">在工作或办公室中使用的桌子，通常用于阅读，写作，或电脑使用。</rdfs:comment>
    <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
    <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://vocab.deri.ie/rooms#Site">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Site</rdfs:label>
    <rdfs:label xml:lang="zh-CN">场地</rdfs:label>
    <rdfs:comment xml:lang="en">An area of land with a designated purpose, such as a university Campus, a housing estate, or a building site.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">带有指定用途的土地区域，如大学校园，一个房屋，或一个建筑工地。</rdfs:comment>
    <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
  </rdfs:Class>

  <rdf:Property rdf:about="http://vocab.deri.ie/rooms#contains">
    <rdfs:label xml:lang="en">Contains</rdfs:label>
    <rdfs:label xml:lang="zh-CN">包括</rdfs:label>
    <rdfs:comment xml:lang="en">The object resource is physically and spatially contained in the subject resource. Being the subject or object of this property implies being a spatial object. Intended for use in the context of buildings, rooms, etc.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">对象资源在物理和空间的意义上包含在主语资源中。作为这个属性的主语或宾语，意味着是一个空间对象。旨在用于建筑、房间等的上下文中。</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://vocab.deri.ie/rooms#occupant">
    <rdfs:label xml:lang="en">Occupant</rdfs:label>
    <rdfs:label xml:lang="zh-CN">居住者</rdfs:label>
    <rdfs:comment xml:lang="en">An agent that generally occupies the physical area of the subject resource. Having this property implies being a spatial object. Being the object of this property implies being an agent. Intended for use with buildings, rooms, desks, etc.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">一个代理，通常占据主体资源的物理区域。有此属性意味着它是一个空间对象。作为这个属性的对象，意味着它是一个代理。旨在和建筑物、房间、桌子等一起使用。</rdfs:comment>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </rdf:Property>

</rdf:RDF>