    <!-- http://purl.org/b2bo#Item -->

    <owl:Class rdf:about="http://purl.org/b2bo#Item">
        <rdfs:label xml:lang="en">Document Item</rdfs:label>
        <rdfs:label xml:lang="pl">Pozycja Dokummentu</rdfs:label>
        <rdfs:comment xml:lang="en">Defines document item that contains information about good/service name, unit of measurement, price.</rdfs:comment>
        <ns:term_status xml:lang="en">stable</ns:term_status>
        <rdfs:comment xml:lang="pl">Definiuje pozycję dokumentu, która zawiera informację m.in. o nazwie towaru/usługi, jednostce miary, cenie.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/b2bo#"/>
    </owl:Class>
    


    <!-- http://purl.org/b2bo#Identifier -->

    <owl:Class rdf:about="http://purl.org/b2bo#Identifier">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/b2bo#prodID"/>
                <owl:onClass rdf:resource="http://purl.org/b2bo#ProductID"/>
                <owl:cardinality>1</owl:cardinality>
