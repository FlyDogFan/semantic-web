        <rdfs:isDefinedBy rdf:resource="http://purl.org/b2bo#"/>
        <rdfs:domain rdf:resource="http://purl.org/b2bo#DocumentAmount"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/b2bo#toPayInWords -->

    <owl:DatatypeProperty rdf:about="http://purl.org/b2bo#toPayInWords">
