        <rdfs:label xml:lang="pl">do zapłaty słownie</rdfs:label>
        <rdfs:comment xml:lang="en">Specifies the value to be paid as prescribed in words.</rdfs:comment>
        <ns:term_status xml:lang="en">stable</ns:term_status>
