        <rdfs:label xml:lang="en">amount of VAT</rdfs:label>
        <rdfs:label xml:lang="pl">kwota VAT</rdfs:label>
        <rdfs:comment xml:lang="en">Specifies the value of the VAT, which is in the amount.</rdfs:comment>
        <ns:term_status xml:lang="en">stable</ns:term_status>
