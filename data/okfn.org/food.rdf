  <rdf:Property rdf:about="http://data.lirmm.fr/ontologies/food#containsIngredient">
    <rdfs:label xml:lang="en-US">contains ingredient</rdfs:label>
    <rdfs:label xml:lang="zh-CN">包含成分</rdfs:label>
