  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/food#Food">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en-US">Food</rdfs:label>
    <rdfs:label xml:lang="zh-CN">食物</rdfs:label>
    <rdfs:comment xml:lang="en-US">Anything that can be eaten: a basic Food item (e.g. salt), a Dish made with a recipe, a manufactured Food Product.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">任何可以吃的：一个基本的食品项（如盐），一盘用于食谱的食物，人工制造的食品。</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/food#FoodProduct">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en-US">Food Product</rdfs:label>
    <rdfs:label xml:lang="zh-CN">食品</rdfs:label>
    <rdfs:comment xml:lang="en-US">A manufactured food product.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">一种人工制造的食品。</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://data.lirmm.fr/ontologies/food#Food"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/food#Ingredient">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en-US">Ingredient</rdfs:label>
    <rdfs:label xml:lang="zh-CN">配料</rdfs:label>
    <rdfs:comment xml:lang="en-US">An ingredient : a certain quantity of food that is part of another food. Represents an ingredient line in a recipe (for a Dish) or an ingredient listed in the ingredient list (for a Product).
      We need to specify a quantity and unit, so the relation is not binary. So the value for the containsIngredient property of a Food object is an object of the IngredientAmount class (instead of a Food object or string directly). See http://www.w3.org/TR/swbp-n-aryRelations/ for details.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN"></rdfs:comment>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/food#NutritionData">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en-US">Nutrition Data</rdfs:label>
    <rdfs:label xml:lang="zh-CN">营养数据</rdfs:label>
    <rdfs:comment xml:lang="en-US">Nutrition data for a food item (recipe, product or ingredient)</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">食物项（食谱，产品或配料）的营养数据。</rdfs:comment>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/food#Dish">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en-US">Dish</rdfs:label>
    <rdfs:label xml:lang="zh-CN">碟</rdfs:label>
    <rdfs:comment xml:lang="en-US">A dish is a food item with a recipe.</rdfs:comment>
    <rdfs:comment xml:lang="zh-CN">一碟菜是食谱中的一个食物项。</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://data.lirmm.fr/ontologies/food#Food"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://data.lirmm.fr/ontologies/food#Recipe">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en-US">Recipe</rdfs:label>
    <rdfs:label xml:lang="zh-CN">食谱</rdfs:label>
    <rdfs:comment xml:lang="en-US">A recipe as a set of ingredients together with the protocol to prepare the resulting food.</rdfs:comment>
