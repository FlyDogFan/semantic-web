  <rdf:Property rdf:about="http://purl.org/NET/biol/ns#hasTaxonomy">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_hasTaxonomy" />
    <dcterms:description xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral" rdf:parseType="Literal">
      This property provides a relationship between an entity (such as a <a xmlns="http://www.w3.org/1999/xhtml" href="http://xmlns.com/foaf/spec/#term_Person">foaf:Person</a>) and a taxonomy. Because biologists don't always agree on how to classify life, a single entity may have multiple taxonomies, each of which may be differentiated between using <a xmlns="http://www.w3.org/1999/xhtml" href="#term_authority">biol:authority</a> or dc:date.
    </dcterms:description>
    <dcterms:description xml:lang="zh-CN" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral" rdf:parseType="Literal">
      
    </dcterms:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type" />
    <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_hasTaxonomy" />
    <rdfs:range rdf:resource="http://purl.org/NET/biol/ns#Taxonomy" />
    <rdfs:label xml:lang="en">taxonomy</rdfs:label>
    <rdfs:label xml:lang="zh-CN">分类</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/NET/biol/ns#kingdom">
    <dcterms:description xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral" rdf:parseType="Literal">
      The highest-level and least specific level of classification. Example kingdoms are <i xmlns="http://www.w3.org/1999/xhtml" class="taxon">Animalia</i> and <i xmlns="http://www.w3.org/1999/xhtml" class="taxon">Plantae</i>.
    </dcterms:description>
    <dcterms:description xml:lang="zh-CN" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral" rdf:parseType="Literal">
      
    </dcterms:description>
    <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_kingdom" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal" />
    <rdfs:label xml:lang="en">kingdom</rdfs:label>
    <rdfs:label xml:lang="zh-CN">界</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_kingdom" />
    <rdfs:domain rdf:resource="http://purl.org/NET/biol/ns#Taxonomy" />
    <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/biol/ns#rank" />
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/NET/biol/ns#morePrecise">
    <dcterms:description xml:lang="en">
      Connects a taxonomy to a more precise taxonomy. For example, the taxonomy
      &#x22;Mammal&#x22; might have a &#x22;more precise&#x22; property linking to &#x22;Primate&#x22;.
    </dcterms:description>
    <dcterms:description xml:lang="zh-CN"></dcterms:description>
    <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_morePrecise" />
    <rdfs:range rdf:resource="http://purl.org/NET/biol/ns#Taxonomy" />
    <rdfs:label xml:lang="en">more precise</rdfs:label>
    <rdfs:label xml:lang="zh-CN">更精确的</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_morePrecise" />
    <rdfs:domain rdf:resource="http://purl.org/NET/biol/ns#Taxonomy" />
    <rdfs:subPropertyOf>
      <rdf:Description rdf:nodeID="Node4">
        <owl:inverseOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf" />
      </rdf:Description>
    </rdfs:subPropertyOf>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/NET/biol/ns#name">
    <dcterms:description xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral" rdf:parseType="Literal">
      A scientific name; a binomial or trinomial. For example, <i xmlns="http://www.w3.org/1999/xhtml" class="taxon">Homo sapiens</i>.
    </dcterms:description>
    <dcterms:description xml:lang="zh-CN" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral" rdf:parseType="Literal">
      
    </dcterms:description>
    <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_name" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal" />
    <rdfs:label xml:lang="en">name</rdfs:label>
    <rdfs:label xml:lang="zh-CN">名称</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_name" />
    <rdfs:domain rdf:resource="http://purl.org/NET/biol/ns#Taxonomy" />
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/NET/biol/ns#order">
    <dcterms:description xml:lang="en">The fourth highest level of classification. For example &#x22;Primates&#x22;.</dcterms:description>
    <dcterms:description xml:lang="zh-CN"></dcterms:description>
    <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_order" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal" />
    <rdfs:label xml:lang="en">order</rdfs:label>
    <rdfs:label xml:lang="zh-CN">目</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_order" />
    <rdfs:domain rdf:resource="http://purl.org/NET/biol/ns#Taxonomy" />
    <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/biol/ns#rank" />
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/NET/biol/ns#phylum">
    <dcterms:description xml:lang="en">
      The second highest level of classification, as used by zoologists. Note that
      phylum may be used to describe any form of life &#x2014; the use of phylum is not
      meant to imply that the entity being described is an animal, merely that
      zoological names of ranks are being used.
    </dcterms:description>
    <dcterms:description xml:lang="zh-CN"></dcterms:description>
    <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_phylum" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal" />
    <rdfs:label xml:lang="en">phylum</rdfs:label>
    <rdfs:label xml:lang="zh-CN">门</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_phylum" />
    <rdfs:domain rdf:resource="http://purl.org/NET/biol/ns#Taxonomy" />
    <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/biol/ns#secondLevelClassification" />
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/NET/biol/ns#rank">
    <dcterms:description xml:lang="en">An unspecified classification. This should not normally be used.</dcterms:description>
    <dcterms:description xml:lang="zh-CN">一个未指定的分类。这通常不应该被使用。</dcterms:description>
    <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_rank" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal" />
    <rdfs:label xml:lang="en">rank</rdfs:label>
    <rdfs:label xml:lang="zh-CN">阶层</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_rank" />
    <rdfs:domain rdf:resource="http://purl.org/NET/biol/ns#Taxonomy" />
    <rdfs:subPropertyOf rdf:resource="urn:lsid:ubio.org:predicates:taxonomicGroup" />
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/NET/biol/ns#secondLevelClassification">
    <dcterms:description xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral" rdf:parseType="Literal">
      The second highest level of classification. Zoologists call these <i xmlns="http://www.w3.org/1999/xhtml">phyla</i> and botanists call them "divisions". In general, you should pick one of those terms and use it instead.
    </dcterms:description>
    <dcterms:description xml:lang="zh-CN" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral" rdf:parseType="Literal">
      
    </dcterms:description>
    <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_secondLevelClassification" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal" />
    <rdfs:label xml:lang="en">second level classification</rdfs:label>
    <rdfs:label xml:lang="zh-CN">第二级分类</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_secondLevelClassification" />
    <rdfs:domain rdf:resource="http://purl.org/NET/biol/ns#Taxonomy" />
    <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/biol/ns#rank" />
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/NET/biol/ns#seeAlso">
    <dcterms:description xml:lang="en">A link to another resource further describing this form of life.</dcterms:description>
    <dcterms:description xml:lang="zh-CN"></dcterms:description>
    <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_seeAlso" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource" />
    <rdfs:label xml:lang="en">see also</rdfs:label>
    <rdfs:label xml:lang="zh-CN">另见</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_seeAlso" />
    <rdfs:domain rdf:resource="http://purl.org/NET/biol/ns#Taxonomy" />
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#seeAlso" />
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/NET/biol/ns#species">
    <dcterms:description xml:lang="en" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral" rdf:parseType="Literal">
      The lowest level of classification supported by this vocabulary. For example <i xmlns="http://www.w3.org/1999/xhtml" class="taxon">Sapiens</i>. To include information more specific than species, use <a xmlns="http://www.w3.org/1999/xhtml" href="#term_rank">biol:rank</a> or <a xmlns="http://www.w3.org/1999/xhtml" href="#section_Abstract">a more specialist vocabulary</a>.
    </dcterms:description>
    <dcterms:description xml:lang="zh-CN" rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral" rdf:parseType="Literal">
      该词汇集支持的最低分类。
    </dcterms:description>
    <xhv:bookmark rdf:resource="http://purl.org/NET/biol/ns.html#term_species" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal" />
    <rdfs:label xml:lang="en">species</rdfs:label>
    <rdfs:label xml:lang="zh-CN">种</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/NET/biol/ns.html#term_species" />
    <rdfs:domain rdf:resource="http://purl.org/NET/biol/ns#Taxonomy" />
    <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/biol/ns#rank" />
  </rdf:Property>

  <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#type">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/rdf-schema/#ch_type" />
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#Class">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/rdf-schema/#ch_class" />
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#Literal">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/rdf-schema/#ch_literal" />
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#Resource">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/rdf-schema/#ch_resource" />
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#label">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/rdf-schema/#ch_label" />
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#seeAlso">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/rdf-schema/#ch_seealso" />
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#subClassOf">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/rdf-schema/#ch_subclassof" />
  </rdf:Description>

</rdf:RDF>